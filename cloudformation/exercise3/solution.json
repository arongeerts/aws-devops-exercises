{
    "Parameters": {
        "MyName": {
            "Type": "String",
            "Default": "aron",
            "Description": "The name of the person creating the S3 Bucket"
        }
    },
    "Resources": {
        "MyBucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {
                "BucketName": {
                    "Fn::Join": [
                        "-",
                        [
                            "aws-devops-session-2",
                            "!Ref MyName",
                            "ex3"
                        ]
                    ]
                }
            }
        }
    }
}